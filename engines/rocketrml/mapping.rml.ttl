@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix fno: <http://w3id.org/function/ontology#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix dc: <http://purl.org/dc/elements/1.1/>.
@prefix rev: <http://purl.org/stuff/rev#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix schema: <http://schema.org/>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#>.
@prefix ex: <http://mapping.example.com/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

ex:map_terms_0 rml:logicalSource ex:source_0.
ex:source_0 a rml:LogicalSource;
    rml:source "data.csv";
    rml:referenceFormulation ql:CSV.
ex:map_terms_0 a rr:TriplesMap;
    rdfs:label "terms".
ex:s_0 a rr:SubjectMap.
ex:map_terms_0 rr:subjectMap ex:s_0.
ex:s_0 rr:template "http://example.org/{id}".
ex:pom_0 a rr:PredicateObjectMap.
ex:map_terms_0 rr:predicateObjectMap ex:pom_0.
ex:pm_0 a rr:PredicateMap.
ex:pom_0 rr:predicateMap ex:pm_0.
ex:pm_0 rr:constant schema:name.
ex:pom_0 rr:objectMap ex:om_0.
ex:om_0 a fnml:FunctionTermMap;
    rr:termType rr:Literal;
    fnml:functionValue ex:fn_0.
ex:fn_0 rml:logicalSource ex:source_0;
    rr:predicateObjectMap ex:pomexec_0.
ex:pomexec_0 rr:predicateMap ex:pmexec_0.
ex:pmexec_0 rr:constant fno:executes.
ex:pomexec_0 rr:objectMap ex:omexec_0.
ex:omexec_0 rr:constant "http://users.ugent.be/~bjdmeest/function/grel.ttl#toUpperCase";
    rr:termType rr:IRI.
ex:fn_0 rr:predicateObjectMap ex:pom_1.
ex:pom_1 a rr:PredicateObjectMap;
    rr:predicateMap ex:pm_1.
ex:pm_1 a rr:PredicateMap;
    rr:constant grel:valueParam.
ex:pom_1 rr:objectMap ex:om_1.
ex:om_1 a rr:ObjectMap;
    rml:reference "value";
    rr:termType rr:Literal.


grel:toUpperCase a fno:Function ;
  fno:name "to Uppercase" ;
  rdfs:label "to Uppercase" ;
  dcterms:description "Returns the input with all letters in upper case." ;
  fno:expects ( grel:valueParam ) ;
  fno:returns ( grel:stringOut ) .

grel:valueParam a fno:Parameter ;
  fno:name "input value" ;
  rdfs:label "input value" ;
  fno:type xsd:string ;
  fno:predicate grel:valueParameter .

grel:stringOut a fno:Output ;
  fno:name "output string" ;
  rdfs:label "output string" ;
  fno:type xsd:string ;
  fno:predicate grel:stringOutput .