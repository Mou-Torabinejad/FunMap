@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms> .
@prefix dce: <http://purl.org/dc/elements/1.1> .
@prefix af: <http://purl.org/ontology/af> .
@prefix ru: <http://purl.org/imbi/ru-meta.owl#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix base: <http://semanticscience.org/resource> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix ex: <http://www.example.com> .


<#TriplesMap1>
	a rr:TriplesMap;

	rml:logicalSource [ rml:source "Source1.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://semanticscience.org/resource/SIO_011137/{B}";
		rr:class sio:SIO_011137 # variation
	];

	rr:predicateObjectMap [ 
    	rr:predicate sio:SIO_001403; 
    	rr:objectMap [
      		rr:parentTriplesMap <TripleMap2>;
      		rr:joinCondition [
        		rr:child <#FunctionMap1>;
        		rr:parent <#FunctionMap2>;
      		];
    	];
  	].

<#TriplesMap2>

	a rr:TriplesMap;
    
  	rml:logicalSource [ rml:source "Source2.csv";
    					rml:referenceFormulation ql:CSV
  						];
  	rr:subjectMap [
    	rr:template "http://project-iasis.eu/Chromosome/{chr}";
    	rr:class iasis:chr
  	]. 

<#FunctionMap1>
	a fnml:FunctionTermMap;

	fnml:functionValue [
        rml:logicalSource [ rml:source "Source1.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:subStringFunction 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:firstInput;
            rr:objectMap [ 
            	rml:reference "D" 
            ]
        ];	
    	rr:predicateObjectMap [
            rr:predicate ex:secondInput;
            rr:objectMap [ 
            	rml:reference "E" 
            ]
        ].

<#FunctionMap2>
	a fnml:FunctionTermMap;

	fnml:functionValue [
        rml:logicalSource [ rml:source "Source1.csv";
		rml:referenceFormulation ql:CSV 
							];
        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:subStringFunction 
        	]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:input;
            rr:objectMap [ 
            	rml:reference "G" 
            ]
        ].



