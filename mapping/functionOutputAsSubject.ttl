@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms> .
@prefix dce: <http://purl.org/dc/elements/1.1> .
@prefix af: <http://purl.org/ontology/af> .
@prefix ru: <http://purl.org/imbi/ru-meta.owl#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix base: <http://semanticscience.org/resource> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sio: <http://semanticscience.org/resource> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <http://semweb.datasciencelab.be/ns/function#> .
@prefix ex: <http://www.example.com> .
	
<#TriplesMap>
	a rr:TriplesMap;

	rml:logicalSource [ rml:source "source1.csv";
						rml:referenceFormulation ql:CSV 
						];

	rr:subjectMap <#FunctionMap>;
	rr:predicateObjectMap [
		rr:predicate rdf:type;  
		rr:objectMap [
			rr:template "http://whatever/{}"
		]
	].

<#FunctionMap>
	a fnml:FunctionTermMap;

	fnml:functionValue [
        rml:logicalSource [ rml:source "source1.csv";
		rml:referenceFormulation ql:CSV 
							];

        rr:predicateObjectMap [
         	rr:predicate fno:executes ;
            rr:objectMap [ 
            	rr:constant ex:subStringFunction 
        	]
        ];
        
        rr:predicateObjectMap [
            rr:predicate ex:stringToLook;
            rr:objectMap [ 
            	rml:reference "hgvs_expressions" 
            ]
        ].
