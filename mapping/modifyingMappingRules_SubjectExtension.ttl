<#TriplesMap2>
	a rr:TriplesMap;

	rml:logicalSource [ rml:source "NewSource2.csv";
						rml:referenceFormulation ql:CSV 
						];
  	rr:subjectMap [
        rr:template "http://whatever/{f(A)}";
        ]
	rr:predicateObjectMap [ 
    	rr:predicate sio:SIO_000255; 
    	rr:objectMap [
      		rr:parentTriplesMap <TripleMap1>; 
      		rr:joinCondition [
        		rr:child "A";             
        		rr:parent "A";            
      		];
    	];
  	];
	rr:predicateObjectMap [ 
    	rr:predicate sio:SIO_001403; 
    	rr:objectMap [
      		rr:parentTriplesMap <TripleMap3>; 
      		rr:joinCondition [
        		rr:child "A";              
        		rr:parent "A";           
      		];
    	];
  	];


