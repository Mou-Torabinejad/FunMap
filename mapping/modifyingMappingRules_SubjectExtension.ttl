<#TriplesMap1>
	a rr:TriplesMap;

	rml:logicalSource [ rml:source "source1.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap <#FunctionMap1>;
	rr:predicateObjectMap [
		rr:predicate rdf:type;  
		rr:objectMap [
			rr:template "http://whatever/{A}"
		]
	]
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000255; # has annotation 
		rr:objectMap [
			rml:reference "B";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate sio:SIO_001403; # is associated with 
		rr:objectMap [
			rml:reference "C";
		]
	].	

