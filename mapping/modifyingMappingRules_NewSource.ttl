<#TriplesMap1>
	a rr:TriplesMap;

	rml:logicalSource [ rml:source "source1.csv";
						rml:referenceFormulation ql:CSV 
						];
  	rr:subjectMap [
        rr:template "http://whatever/{}";
        rr:class "http://whatever/"
        ]  
	rr:predicateObjectMap [
		rr:predicate sio:SIO_000255; # has annotation 
		rr:objectMap <#FunctionMap2>
	];	
	rr:predicateObjectMap [
		rr:predicate owl:sameAs; 
		rr:template "http://something/{}"
	].

