<#TriplesMap8>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source <#DB_source>;
                        rr:tableName "PROJECT1";];
    rr:subjectMap [
        rr:template "http://project-iasis.eu/Gene/{Gene name}";
    ];
    rr:predicateObjectMap [
        rr:predicate iasis:isRelatedTo;
        rr:objectMap [
        rr:parentTriplesMap <#OUTPUT1>;
        rr:joinCondition [
            rr:child "Mutation CDS";
            rr:parent "Mutation CDS";
            ];
        rr:joinCondition [
            rr:child "Gene name";
            rr:parent "Gene name";
            ];
        ];
    ].

