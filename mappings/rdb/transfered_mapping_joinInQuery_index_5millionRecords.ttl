<#TriplesMap8>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source <#DB_source>;
                        rr:tableName "PROJECT1","OUTPUT1";
                        rml:query "SSELECT DISTINCT PROJECT1.`Gene name`, OUTPUT1.`OUTPUT1` FROM PROJECT1 USE INDEX (p1), OUTPUT1 USE INDEX (o1) WHERE PROJECT1.`Gene name`=OUTPUT1.`Gene name`and PROJECT1.`Mutation CDS`=OUTPUT1.`Mutation CDS`";];
    rr:subjectMap [
        rr:template "http://project-iasis.eu/Gene/{Gene name}";
    ];
    rr:predicateObjectMap [
        rr:predicate iasis:isRelatedTo;
        rr:objectMap [
        rml:reference "OUTPUT1"
        ]
    ].

