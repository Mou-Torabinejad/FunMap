@prefix fno: <http://semweb.datasciencelab.be/ns/function#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://www.example.com> .

ex:subStringProblem a fno:Problem;
ex:subStringAlgorithm a fno:Algorithm;

ex:subStringFunction a fno:Function;
    fno:name "The sub-string extraction function"^^xsd:string;
    dcterms:description "This function can extract part of a string base on requirement."^^xsd:string;
    fno:solves ex:subStringProblem;
    fno:implements ex:subStringAlgorithm;
    fno:expects ( 
        [
            fno:predicate ex:queryString; 
            fno:required "true"^^xsd:boolean 
        ]
        [
            fno:predicate ex:subString; 
            fno:required "true"^^xsd:boolean 
        ]
    ).
    fno:returns (
        [
            fno:predicate ex:output; 
            fno:required "true"^^xsd:boolean 
        ]  
    ).
ex:queryString fno:type xsd:string.
ex:subString fno:type xsd:string.
ex:extractedSubString fno:type xsd:string.

ex:subStringExecution a fno:Execution;
    fno:executes ex:subStringFunction;
    ex:queryString "???"^^xsd:string;
    ex:subString "NC"^^xsd:string.

