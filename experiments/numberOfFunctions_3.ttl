@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <http://semweb.datasciencelab.be/ns/function#> .
@prefix ex: <http://www.example.com/> .
@base <http://MapFun.KG/> .

<#TriplesMap1>
    rml:logicalSource [ rml:source "data.csv";
                        rml:referenceFormulation ql:CSV ];
    rr:subjectMap [
        rr:template "http://project-iasis.eu/Mutation/{GENOMIC_MUTATION_ID}";
        rr:class iasis:Mutation;
    ];
    rr:predicateObjectMap [
        rr:predicate iasis:geneName;
        rr:objectMap [ 
            rml:reference "Gene name"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate iasis:sample;
        rr:objectMap [ 
            rml:reference "Sample name" 
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate iasis:tissue;
        rr:objectMap <#FunctionMap3>
    ].


<#TriplesMap2>
    rml:logicalSource [ rml:source "data.csv";
                        rml:referenceFormulation ql:CSV ];
    rr:subjectMap [
        rr:template "http://project-iasis.eu/GenomicPosition/{cFormat}";
        rr:class iasis:GenomicPosition;
    ];  

    rr:predicateObjectMap [
        rr:predicate iasis:mutation_cds;
        rr:objectMap <#FunctionMap1>
    ];
    rr:predicateObjectMap [
        rr:predicate iasis:chromosome_start_end;
        rr:objectMap <#FunctionMap2>
    ].  

<#FunctionMap1>
    a fnml:FunctionTermMap;

    fnml:functionValue [
        rml:logicalSource [ rml:source "data.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:replaceValue 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
                rml:reference "Mutation CDS" 
            ]
        ];  
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
                rr:constant ">"; 
            ]
        ];  
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
                rr:constant "_"; 
            ]
        ]; 
        ].

<#FunctionMap2>
    a fnml:FunctionTermMap;

    fnml:functionValue [
        rml:logicalSource [ rml:source "data.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:replaceValue 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
                rml:reference "Mutation genome position" 
            ]
        ];  
        rr:predicateObjectMap [
            rr:predicate ex:value2;
            rr:objectMap [ 
                rr:constant "-"; 
            ]
        ];  
        rr:predicateObjectMap [
            rr:predicate ex:value3;
            rr:objectMap [ 
                rr:constant ":"; 
            ]
        ]; 
        ]. 

<#FunctionMap3>
    a fnml:FunctionTermMap;

    fnml:functionValue [
        rml:logicalSource [ rml:source "data.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant ex:tolower 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate ex:value;
            rr:objectMap [ 
                rml:reference "Primary site" 
            ]
        ];    
        ].                
